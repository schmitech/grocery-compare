# OpenAI API key
OPENAI_API_KEY=your_openai_api_key

# Google Gemini API key
GOOGLE_API_KEY=your_google_api_key

# Default AI provider (options: "openai", "google", "auto")
# "auto" will try OpenAI first, then Google if OpenAI is not available
# You can override this in the UI by selecting a different provider
DEFAULT_AI_PROVIDER=google
GOOGLE_GENAI_MODEL=gemini-2.0-flash
OPEN_AI_MODEL=gpt-4o-mini